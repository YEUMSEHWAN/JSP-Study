package com.boardone;

public class BoardDAO {

	private static BoardDAO instance = null;
	
	private BoardDAO() {}

	public static BoardDAO getInstance() {
		
		if(instance == null) {
			synchronized (BoardDAO.class) {
				instance = new BoardDAO();
			}
		}
		return instance;
	}
	
	//이곳에서 부터 게시판 작업 기능을 하나씩 메소드를 추가해 작성하면 됨.
	
}
